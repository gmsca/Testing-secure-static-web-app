{
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
      },

    "routes": [
        {
          "route": "/*",
          "allowedRoles": ["authenticated"]
        },

        {
            "route": "/index.html",
            "headers": {
              "cache-control": "must-revalidate, max-age=15770000"
            }
          }

      ],
      "responseOverrides": {
        "401": {
          "statusCode": 302,
          "redirect": "/.auth/login/aadb2c?post_login_redirect_uri=.referrer"
        }
      },  


    "navigationFallback": {
        "rewrite": "/Naverror.html"
    },

    "auth": {
        "identityProviders": {
          "customOpenIdConnectProviders": {
            "aadb2c": {
              "registration": {
                "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
                "clientCredential": {
                  "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
                },
                "openIdConnectConfiguration": {
                  "wellKnownOpenIdConfiguration": "https://devb2chmc.b2clogin.com/devb2chmc.onmicrosoft.com/B2C_1_signin1/v2.0/.well-known/openid-configuration"
                }
              },
              "login": {
                "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                "scopes": [],
                "loginParameterNames": []
              }
            }
          }
        }
      }
}